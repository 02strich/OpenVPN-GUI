    LTEXT           "Password:", 202, 9, 26, 38, 10
    EDITTEXT        EDIT_PROXY_PASSWORD, 49, 23, 94, 12, ES_PASSWORD | ES_AUTOHSCROLL
    PUSHBUTTON      "OK", IDOK, 58, 43, 40, 14
END

